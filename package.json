{
  "name": "aura-challenge-selenium_account",
  "version": "0.0.0",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@sitespeed.io/chromedriver": "^92.0.4515-43",
    "@types/jest": "^27.0.1",
    "prettier": "^2.2.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "ts-node": "^10.2.0"
  },
  "jest": {
    "rootDir": "./src",
    "testMatch": [
      "**/**/*.js",
      "**/*.js",
      "**/**/*.ts",
      "**/*.ts"
    ],
    "testTimeout": 90000,
    "testEnvironment": "node",
    "maxConcurrency": 3,
    "verbose": true
  },
  "scripts": {
    "format:fix": "prettier --write src/**/*.ts",
    "test": "jest"
  }
}
